{
  "name": "meal-planner-agent",
  "version": "2.0.0",
  "description": "AI-powered meal planning web application with Claude AI - Monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @meal-planner/web dev",
    "dev:worker": "pnpm --filter @meal-planner/queue dev",
    "db:migrate": "pnpm --filter @meal-planner/database migrate",
    "db:studio": "pnpm --filter @meal-planner/database studio",
    "db:seed": "pnpm --filter @meal-planner/database seed"
  },
  "keywords": [
    "meal-planner",
    "claude",
    "ai-agent",
    "nextjs",
    "web-app"
  ],
  "author": "",
  "license": "MIT"
}
