{
  "name": "meal-planner-agent",
  "version": "1.0.0",
  "description": "Weekly meal planning agent with Claude AI, email notifications, and HEB ordering",
  "main": "dist/index.js",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test:now": "ts-node src/index.ts --now",
    "test:now:send": "ts-node src/index.ts --now --sendemail",
    "test:heb-scraper": "ts-node tests/heb-scraper/test-heb-scraper.ts",
    "test:heb-connector": "ts-node tests/heb-scraper/test-connector-only.ts",
    "test:email-send": "ts-node tests/test-email-send.ts",
    "test:meal-history": "ts-node tests/test-meal-history.ts",
    "watch": "tsc --watch"
  },
  "keywords": [
    "meal-planner",
    "claude",
    "ai-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "dotenv": "^16.4.7",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "puppeteer": "^23.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
