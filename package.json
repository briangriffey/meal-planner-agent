{
  "name": "meal-planner-agent",
  "version": "2.0.0",
  "description": "AI-powered meal planning web application with Claude AI - Monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @meal-planner/web dev",
    "dev:worker": "pnpm --filter @meal-planner/queue dev",
    "db:migrate": "pnpm --filter @meal-planner/database migrate",
    "db:migrate:deploy": "pnpm --filter @meal-planner/database migrate:deploy",
    "db:generate": "pnpm --filter @meal-planner/database generate",
    "db:studio": "pnpm --filter @meal-planner/database studio",
    "db:push": "pnpm --filter @meal-planner/database push",
    "db:seed": "pnpm --filter @meal-planner/database seed",
    "test:agent": "ts-node scripts/test-agent.ts"
  },
  "keywords": [
    "meal-planner",
    "claude",
    "ai-agent",
    "nextjs",
    "web-app"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "dotenv": "^16.4.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
